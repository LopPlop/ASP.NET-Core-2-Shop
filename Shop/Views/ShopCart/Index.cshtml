@model ShopCartViewModel
<div class="container">
    @foreach(var el in Model._shopCart.listShopItems){
        <div class="alert alert-warning mt-3">
            <b>Автомобиль:</b> @el._car.name<br />
            <b>Цена:</b> @el._car.price.ToString("c")
        </div>
    }
    <hr />
    <div class="btn btn-danger" asp-controller="Order">Оплатить</div>
</div>